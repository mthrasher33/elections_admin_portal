<div class="candidateDetail">
   <div class="col-xs-12">
      <div class="row">
         <h3> <%=data[0][0].First %> <%=data[0][0].Last%> </h3>
         <hr>
      </div>
   <div class="row">
      <div class="col-xs-12 col-sm-6">
            <span class="fa fa-institution"></span><span>Races: 
         	<ul>
         <% for(var i=0; i < data[0].length; i++) { %>
            <li><span> <%= data[0][i].Date %> <%= data[0][i].Name%> </span>
            <% if(data[0][i].City) { %>
            	 <span>  <%= data[0][i].City %> </span>
            <% } else { %>
            	 <span>  <%= data[0][i].Ward_Name %> </span>
            <% } %>
               <% if(data[0][i].wonRace) { %>
            	 <span>  (Won) </span>
            <% } else { %>
            	 <span>  (Lost) </span>
            <% } %>
            </li>
         <% } %>
         	</ul>
         </span>


         <span class="fa fa-phone"></span><span>Phone:  

         <% if(data[0][0].Phone) { %>
         <%=data[0][0].Phone %> 
            <% } else { %>
            Unknown
            <% } %>
         </span>

         <br>

         <span class="fa fa-fax"></span><span>Fax:  

         <% if(data[0][0].Fax) { %>
         <%=data[0][0].Fax %> 
            <% } else { %>
            Unknown
            <% } %>
         </span>

         <br>

         <span class="fa fa-envelope"></span><span>Email:  


         <% if(data[0][0].Email) { %>
         <%=data[0][0].Email %> 
            <% } else { %>
            Unknown
            <% } %>
         </span>
         

    
         </div>

      <div class="col-xs-12 col-sm-6">
             <span class="fa fa-globe"></span><span>Website:  

         <% if(data[0][0].Website) { %>
         <a href = "<%=data[0][0].Website %>" target="_blank"> Link </a> 
            <% } else { %>
            Unknown
            <% } %>
         </span>  

         <br>

         <span class="fa fa-youtube"></span><span>Youtube:  

         <% if(data[0][0].Youtube) { %>
         <a href = "<%=data[0][0].Website %>" target="_blank"> Link </a> 
            <% } else { %>
            Unknown
            <% } %>
         </span> 
         <br> 

         <span class="fa fa-facebook"></span><span>Facebook:  


         <% if(data[0][0].Facebook) { %>
         <a href = "<%=data[0][0].Facebook %>" target="_blank"> Link </a> 
            <% } else { %>
            Unknown
            <% } %>
         </span> 
         <br> 


         <span class="fa fa-twitter"></span><span>Twitter:  


         <% if(data[0][0].Twitter) { %>
         <a href = "<%=data[0][0].Website %>" target="_blank"> Link </a> 
            <% } else { %>
            Unknown
            <% } %>
         </span> 
         <br>


         <span class="fa fa-linkedin"></span><span>LinkedIn:  
         

         <% if(data[0][0].linkedIn) { %>
         <a href = "<%=data[0][0].linkedIn %>" target="_blank"> Link </a> 
            <% } else { %>
            Unknown
            <% } %>
         </span>       </div>
   </div>
   </div>
</div>
<script>
</script>